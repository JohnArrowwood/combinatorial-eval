{
  "name": "combinatorial-eval",
  "version": "1.0.0",
  "description": "Generate every combination of possible values for some set of parameters",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "nyc mocha test/*.test.ts test/**/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JohnArrowwood/combinatorial-eval.git"
  },
  "author": "John Arrowwood",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JohnArrowwood/combinatorial-eval/issues"
  },
  "homepage": "https://github.com/JohnArrowwood/combinatorial-eval#readme",
  "devDependencies": {
    "@types/chai": "^4.0.8",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "source-map-support": "^0.5.0",
    "ts-node": "^4.0.1",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "progressive-eval": "^1.2.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99
  }
}
